<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hfoa.dao.user.DepartmentMapper">
  
   <select id="selectDuty"  resultType="com.hfoa.entity.user.DepartmentEntity">
  	select * from department 
  </select>
   
   <select id="getByName" parameterType="java.lang.String" resultType="com.hfoa.entity.user.DepartmentEntity">
   	select * from department where DepartmentName=#{departName}
   </select>


</mapper>